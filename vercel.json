{
  "installCommand": "npm install --ignore-scripts",
  "buildCommand": "echo 'skip'",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/pkg.tgz",
      "destination": "/api/pkg.ts"
    },
    {
      "source": "/((?!(?:about|api)/)(?:[^?]+/)+[^?]+)(\\?.+)?",
      "destination": "/api/pkg-from-url"
    }
  ]
}
